{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "framework": "vite",
  "outputDirectory": "dist",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "@node_env",
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret",
    "SESSION_SECRET": "@session_secret",
    "VITE_APP_URL": "@vite_app_url",
    "VITE_APP_TITLE": "@vite_app_title",
    "VITE_APP_LOGO": "@vite_app_logo",
    "REDIS_URL": "@redis_url",
    "RESEND_API_KEY": "@resend_api_key",
    "RESEND_FROM_EMAIL": "@resend_from_email",
    "OPENAI_API_KEY": "@openai_api_key",
    "AWS_ACCESS_KEY_ID": "@aws_access_key_id",
    "AWS_SECRET_ACCESS_KEY": "@aws_secret_access_key",
    "AWS_REGION": "@aws_region",
    "AWS_S3_BUCKET": "@aws_s3_bucket",
    "VITE_SENTRY_DSN": "@vite_sentry_dsn",
    "SENTRY_AUTH_TOKEN": "@sentry_auth_token",
    "MOYASAR_API_KEY": "@moyasar_api_key",
    "MOYASAR_SECRET_KEY": "@moyasar_secret_key",
    "TAP_API_KEY": "@tap_api_key",
    "TAP_SECRET_KEY": "@tap_secret_key",
    "SMS_API_KEY": "@sms_api_key",
    "SMS_SENDER_ID": "@sms_sender_id",
    "GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
    "VITE_APP_ID": "@vite_app_id",
    "OAUTH_SERVER_URL": "@oauth_server_url",
    "VITE_OAUTH_PORTAL_URL": "@vite_oauth_portal_url",
    "BUILT_IN_FORGE_API_URL": "@built_in_forge_api_url",
    "BUILT_IN_FORGE_API_KEY": "@built_in_forge_api_key",
    "VITE_FRONTEND_FORGE_API_KEY": "@vite_frontend_forge_api_key",
    "VITE_FRONTEND_FORGE_API_URL": "@vite_frontend_forge_api_url",
    "OWNER_OPEN_ID": "@owner_open_id",
    "OWNER_NAME": "@owner_name"
  },
  "functions": {
    "server/_core/index.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/server/_core/index.ts"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "/server/_core/index.ts"
    },
    {
      "source": "/api/:path*",
      "destination": "/server/_core/index.ts"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
