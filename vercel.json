{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "framework": "vite",
  "ignoreCommand": "[[ \"$VERCEL_GIT_COMMIT_REF\" != \"main\" ]] && echo 'skip'",
  "env": {},
  "functions": {
    "server/_core/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/server/_core/index.ts"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "/server/_core/index.ts"
    }
  ]
}
